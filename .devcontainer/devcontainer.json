{
  "name": "Flutter Codespace",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers/features/flutter:1": {},
    "ghcr.io/firebase/firebase-tools/stable:1": {}
  },
  "forwardPorts": [
    8080
  ],
  "postCreateCommand": "flutter precache --web",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  },
  "remoteUser": "vscode"
}
